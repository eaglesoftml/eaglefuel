<!--  vue formulaire de la classe station  -->
<record model="ir.ui.view" id="station_form_view">
     <field name="name">eaglefuel.station.form</field>
     <field name="model">eaglefuel.station</field>
     <field name="arch" type="xml">
             <form string="Station">
                  <header>
                  </header>
                  <sheet>
                       <div class="oe_title oe_left">
                            <div class="oe_edit_only">
                                 <label for="name" string="Nom"/>
                            </div>
                            <h1>
                                 <field name="name" readonly="0" placeholder="Station"/>
                            </h1>
                       </div>
                            <group>
                                 <group>
                                      <field name="adress" />
                                      <field name="description" />
                                 </group>
                            </group>
                                <notebook>
                                    <page string="La liste des ilos">
                                        <field name="ilo_id" />
                                    </page>
                                    <page string="La liste des cuves">
                                        <field name="cuve_id" />
                                    </page>
                                    <page string="La liste des employes">
                                        <field name="employe_id" />
                                    </page>
                                 </notebook>
                  </sheet>
             </form>
     </field>
</record>

<record model="ir.ui.view" id="station_tree_view">
     <field name="name">eaglefuel.station.form</field>
     <field name="model">eaglefuel.station</field>
     <field name="arch" type="xml">
         <tree string="station">
            <field name="name" />
            <field name="adress" />
            <field name="description" />
            <field name="ilo_id" />
        </tree>
     </field>
</record>

<record model='ir.actions.act_window' id='station_action'>
       <field name="name">Stations</field>
       <field name="res_model">eaglefuel.station</field>
       <field name="view_mode">tree,form,kanban</field>
       <field name="help" type ="html">
            <p class="o_view_nocontent_smiling_face"></p>
       </field>
</record>

<menuitem name="Gas Station" id="station_root" sequence="7"/>
<menuitem name="Stations" id="sub_station_root" parent="station_root"/>
<menuitem name="Station" id="sub_sub_station_root" parent="sub_station_root" action="station_action"/>


<record model="ir.ui.view" id="ilo_form_view">
     <field name="name">eaglefuel.ilo.form</field>
     <field name="model">eaglefuel.ilo</field>
     <field name="arch" type="xml">
             <form string="ilo">
                  <header>
                  </header>
                 <sheet>
                     <group>
                      <group>
                          <field name="number"/>
                          <field name="nom"/>
                          <field name="description"/>
                      </group>
                  </group>
                     <notebook>
                         <page string="Station parent">
                           <field name="station_id"/>
                         </page>
                         <page string="Liste des pompes">
                             <field name="pompe_id"/>
                         </page>
                     </notebook>
                 </sheet>
             </form>
     </field>
</record>

<record model="ir.ui.view" id="ilo_tree_view">
     <field name="name">eaglefuel.ilo.form</field>
     <field name="model">eaglefuel.ilo</field>
     <field name="arch" type="xml">
             <tree string="Ilo">
                 <field name="number"/>
                 <field name="nom"/>
                 <field name="description"/>
                 <field name="description"/>
                 <field name="station_id"/>
                 <field name="pompe_id"/>
             </tree>
     </field>
</record>

<record model='ir.actions.act_window' id='ilo_action'>
       <field name="name">ilo</field>
       <field name="res_model">eaglefuel.ilo</field>
       <field name="view_mode">tree,form,kanban</field>
       <field name="help" type ="html">
            <p class="o_view_nocontent_smiling_face"></p>
       </field>
</record>


<menuitem name="ilo" id="sub_ilo_root" parent="station_root"/>

<menuitem name="ilo" id="sub_sub_ilo_root" parent="sub_ilo_root" action="ilo_action"/>

<record model="ir.ui.view" id="pompe_form_view">
    <field name="name">eaglefuel.pompe.form</field>
    <field name="model">eaglefuel.pompe</field>
